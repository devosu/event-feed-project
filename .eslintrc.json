{
  "root": true,
  "env": {
    "es6": true,
    "jest": true,
    "node": true,
    "browser": true
  },
  "parser": "@babel/eslint-parser",
  "plugins": [
    "prettier"
  ],
  "extends": [
    "prettier",
    "react-app",
    "react-app/jest",
    "eslint:recommended"
  ],
  "ignorePatterns": [
    "node_modules/",
    "build/"
  ],
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "prettier/prettier": 2
  }
}